#include <iostream>

class Rectangle {
private:
    double length;
    double width;

public:
    // Constructor with default values for length and width
    Rectangle(double len = 1.0, double wid = 1.0) : length(len), width(wid) {}

    // Constructor with parameters to set the length and width
    Rectangle(double len, double wid) : length(len), width(wid) {}

    // Method to get the dimensions of the rectangle
    void getDimensions() const {
        std::cout << "Length: " << length << ", Width: " << width << std::endl;
    }
};

int main() {
    // Creating objects of the Rectangle class using both constructors
    Rectangle defaultRect; // Object with default values
    Rectangle customRect(5.0, 3.0); // Object with custom values

    // Displaying dimensions of both rectangles
    std::cout << "Default Rectangle Dimensions: ";
    defaultRect.getDimensions();
    std::cout << "Custom Rectangle Dimensions: ";
    customRect.getDimensions();

    return 0;
}
