//Function to count the no of elements in a string

#include<iostream>
using namespace std;
int count(string word){
    int count = 0;
    for(int i=0;i<word.size();i++){
        count++;
    }
    return count;
}
int main()
{

    string sentence;
    getline(cin,sentence);

    cout<<"Number of elements in the string are "<<count(sentence);
    return 0;
}
