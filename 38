//sum of elements in an array

#include<iostream>
using namespace std;
int main()
{
    int n,num,sum=0;
    cout<<"Enter number of elements: "<<endl;
    cin>>n;

    int array[n];
    cout<<"Enter elements: \n";
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    cout<<"Sum is: ";
    for(int i=0;i<n;i++){
        sum = sum + array[i];
    }
    cout<<sum;

    return 0;
}
