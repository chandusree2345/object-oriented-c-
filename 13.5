#include <iostream>

int main() {
    // Declare and initialize two integer variables
    int firstInteger = 10;
    int secondInteger = 20;

    // Perform addition using the + operator
    int result = firstInteger + secondInteger;

    // Output the result
    std::cout << "The sum of " << firstInteger << " and " << secondInteger << " is: " << result << std::endl;

    return 0;
}
