
// prime number using functions

#include<iostream>
using namespace std;
int prime(int n){
    for(int i=2;i<n-1;i++){
        if(n%i==0){
            return 1;
        }
    }
    return 2;
}
int main()
{
    cout<<"Enter the number"<<endl;
    int num;
    cin>>num;

    string result = (prime(num)==2)?"Prime Number":"Not a Prime Number";
    cout<<result;

    return 0;
}
