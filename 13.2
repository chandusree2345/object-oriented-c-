#include <iostream>

int main() {
    // Integer data type
    int integerNumber = 10;

    // Float data type
    float floatNumber = 3.14;

    // Character data type
    char character = 'A';

    // Output the values
    std::cout << "Integer Number: " << integerNumber << std::endl;
    std::cout << "Float Number: " << floatNumber << std::endl;
    std::cout << "Character: " << character << std::endl;

    return 0;
}
