#include <iostream>

int main() {
    int num1, num2, result;

   
    std::cout << "Enter the first integer: ";
    std::cin >> num1;

   
    std::cout << "Enter the second integer: ";
    std::cin >> num2;

   
    result = num1 * num2;

    
    std::cout << "The result of multiplication  is: " << result << std::endl;

    return 0;
}
