//reverse a array

#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter Input: "<<endl;
    cin>>n;
    int array[n];

    cout<<"Enter elements: "<<endl;
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    cout<<"Reverse Elements: "<<endl;
    for(int i=n-1;i>=0;i--){
        cout<<array[i]<<" ";
    }

    return 0;
}
